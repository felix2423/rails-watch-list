<div class="container bg-light p-4 mt-4 rounded">
  <h1>Create a new bookmark</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit %>
  <% end %>
  <br>
  <%= link_to 'Back', list_path(@list), class: "btn btn-dark"  %>
</div>
